#model_data_container
	div.model_data_element
		label._inline id:
			a(data-model-attr="_id")
	#model_data_attributes
	#model_data_controls
		input#model_data_save._inline(type="button", value="Save");
		input#model_data_new._inline(type="button", value="Save New Model");
		input#model_data_remove._inline(type="button", value="Remove Model");
	#model_data_status